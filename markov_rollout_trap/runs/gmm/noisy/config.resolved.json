{
  "dataloader": {
    "batch_size": 256,
    "drop_last": false,
    "num_workers": 0,
    "pin_memory": false,
    "shuffle": true
  },
  "model": {
    "gmm": {
      "dummy": 0
    },
    "latent": {
      "elbo_samples": 1,
      "emb_dim": 8,
      "encoder_hidden": 64,
