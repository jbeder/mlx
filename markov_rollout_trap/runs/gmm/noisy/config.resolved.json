    "markov": {
      "emb_dim": 8,
      "num_components": 2
    }
  },
  "optim": {
    "lr": 0.0003,
    "weight_decay": 0.0
  },
  "train": {
    "epochs": 50,
    "grad_clip": 0.0
  }
}